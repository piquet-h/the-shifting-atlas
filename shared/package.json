{
    "name": "@atlas/shared",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./events": {
            "import": "./dist/events/worldEventSchema.js",
            "types": "./dist/events/worldEventSchema.d.ts",
            "require": "./dist/events/worldEventSchema.js"
        }
    },
    "browser": {
        "./dist/index.js": "./dist/index.browser.js"
    },
    "files": [
        "dist"
    ],
    "engines": {
        "node": "20.x",
        "npm": ">=10.0.0"
    },
    "scripts": {
        "clean": "node ../scripts/clean.mjs --here",
        "build": "tsc -p tsconfig.json",
        "watch": "tsc -p tsconfig.json --watch --preserveWatchOutput",
        "typecheck": "tsc -p tsconfig.json --noEmit",
        "test": "node --test --import=tsx test/*.test.ts"
    },
    "dependencies": {
        "@azure/identity": "^4.12.0",
        "@azure/keyvault-secrets": "^4.10.0",
        "gremlin": "^3.7.0",
        "zod": "^4.1.11"
    },
    "devDependencies": {
        "@types/gremlin": "^3.6.5",
        "typescript": "^5.9.2"
    }
}
