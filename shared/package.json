{
    "name": "@atlas/shared",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "engines": {
        "node": "20.x",
        "npm": ">=10.0.0"
    },
    "scripts": {
        "clean": "node ../scripts/clean.mjs --here",
        "build": "tsc -p tsconfig.json",
        "watch": "tsc -p tsconfig.json --watch --preserveWatchOutput",
        "typecheck": "tsc -p tsconfig.json --noEmit",
        "test": "node --test --import=tsx test/*.test.ts"
    },
    "dependencies": {
        "gremlin": "^3.7.0"
    },
    "devDependencies": {
        "typescript": "^5.9.2",
        "@types/gremlin": "^3.6.5"
    }
}
