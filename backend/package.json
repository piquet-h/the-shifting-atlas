{
    "name": "the-shifting-atlas-backend",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "watch": "tsc --watch --preserveWatchOutput",
        "typecheck": "tsc --noEmit",
        "test": "node --test dist",
        "dev": "tsc -w --preserveWatchOutput & func start",
        "start": "npm run build && func start"
    },
    "engines": {
        "node": "20.x",
        "npm": ">=10.0.0"
    },
    "dependencies": {
        "@azure/functions": "^4.8.0",
        "@atlas/shared": "file:../shared",
        "applicationinsights": "^2.9.5"
    }
}
