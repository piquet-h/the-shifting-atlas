{
    "version": "2.0",
    "extensionBundle": {
        "id": "Microsoft.Azure.Functions.ExtensionBundle",
        "version": "[4.*, 5.0.0)"
    },
    "logging": {
        "console": {
            "isEnabled": true,
            "disableColors": false
        },
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": false
            }
        },
        "logLevel": {
            "default": "Trace"
        }
    }
}
